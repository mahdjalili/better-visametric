{
    "name": "Better Visametic",
    "version": "1.0",
    "description": "Extension!",
    "permissions": ["activeTab", "tabs", "scripting", "storage"],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["jquery-3.5.1.min.js", "script.js"]
        }
    ],
    "manifest_version": 3
}
